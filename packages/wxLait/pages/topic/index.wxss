.tab-h {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: white;
  font-size: 16px;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  padding-right: 30px;
  z-index: 99;
}

.tab-item {
  margin: 0 15rpx;
  color: #b9b9b9;
  font-size: 34rpx;
  display: inline-block;
}

.tab-item.active {
  color: #333;
  font-size: 34rpx;
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 8rpx;
  width: 52rpx;
  background: #333;
  position: absolute;
  bottom: 0;
  left: 5rpx;
  border-radius: 16rpx;
}
.tag-top{
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
}

.item-image{
  width: 30px;
  height: 40px;
}

.image-p{
  display: flex;
  flex-direction: column;
}
.img-search{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 25px;
  height: 25px;
}
.search-p{
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  right: 0px;
  top: 0px;
  height: 80rpx;
  width: 35px;
  background-color: white;
}